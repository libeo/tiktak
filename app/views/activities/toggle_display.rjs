if session[:collapse_projects][params[:id]].to_i == 1
  page["projects_customer_#{params[:id]}"].hide
  page["indicator_#{params[:id]}"].replace_html(image_tag('collapsed.png', :border => 0))
else
  page["projects_customer_#{params[:id]}"].show
  page["indicator_#{params[:id]}"].replace_html(image_tag('expanded.png', :border => 0))
end
